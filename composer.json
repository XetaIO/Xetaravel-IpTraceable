{
    "name": "xetaio/xetaravel-iptraceable",
    "description": "A simple package that update an IP field when the user login into the application.",
    "keywords": [
        "laravel",
        "ip",
        "traceable",
        "users",
        "login"
    ],
    "homepage": "https://github.com/XetaIO/Xetaravel-IpTraceable",
    "support": {
        "source": "https://github.com/XetaIO/Xetaravel-IpTraceable",
        "issues": "https://github.com/XetaIO/Xetaravel-IpTraceable/issues"
    },
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Xety",
            "email": "zoro.fmt@gmail.com",
            "homepage": "https://github.com/Xety"
        }
    ],
    "require": {
        "php": ">=8.1",
        "illuminate/auth": "^10.0",
        "illuminate/contracts": "^10.0",
        "illuminate/database": "^10.0",
        "illuminate/http": "^10.0",
        "illuminate/support": "^10.0",
        "nesbot/carbon": "^2.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.2"
    },
    "autoload": {
        "psr-4": {
            "Xetaio\\IpTraceable\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
